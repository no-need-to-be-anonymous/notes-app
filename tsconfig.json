{
   "compilerOptions": {
      "module": "commonjs",
      "target": "esnext",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "allowJs": true,
      "lib": ["dom", "es6"],
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "types": ["reflect-metadata", "swagger-express-ts", "jest"]
   },
   "exclude": ["node_modules", "dist"]
}

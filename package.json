{
   "name": "notes-app",
   "version": "1.0.0",
   "main": "index.ts",
   "repository": "https://github.com/no-need-to-be-anonymous/notes-app.git",
   "author": "Oleg Antony√°k <oantonyak@gmail.com>",
   "license": "MIT",
   "scripts": {
      "dev": "nodemon index.ts",
      "build": "tsc",
      "start": "node dist/index.js",
      "prisma:migrate": "npx prisma migrate deploy",
      "prisma:migrate-test": "npx prisma migrate deploy",
      "prettier:format": "prettier --write .",
      "eslint:format": "eslint . --ext .ts,.js",
      "docker:up": "docker compose up",
      "docker:up-rebuild": "docker compose down; docker compose up --renew-anon-volumes --build --force-recreate backend",
      "docker:up-test": "docker compose -f docker-compose.test.yml up --renew-anon-volumes --build --force-recreate",
      "docker:down": "docker compose down;",
      "test": "yarn run test:integration -- --watch=false && yarn run test:e2e -- --watch=false",
      "test:integration": "jest --config ./jest.config.ts --watch",
      "test:e2e": "jest --config ./jest-e2e.config.ts"
   },
   "devDependencies": {
      "@babel/core": "^7.19.3",
      "@babel/plugin-proposal-class-properties": "^7.18.6",
      "@babel/plugin-proposal-decorators": "^7.19.3",
      "@babel/plugin-syntax-decorators": "^7.19.0",
      "@babel/preset-env": "^7.19.3",
      "@babel/preset-typescript": "^7.18.6",
      "@types/express": "^4.17.14",
      "@types/jest": "^29.1.1",
      "@types/supertest": "^2.0.12",
      "@types/swagger-jsdoc": "^6.0.1",
      "@types/swagger-ui-express": "^4.1.3",
      "@typescript-eslint/eslint-plugin": "^5.37.0",
      "@typescript-eslint/parser": "^5.37.0",
      "babel-jest": "^29.1.2",
      "babel-plugin-parameter-decorator": "^1.0.16",
      "eslint": "^8.23.1",
      "inversify": "^6.0.1",
      "inversify-express-utils": "^6.4.3",
      "jest": "^29.1.2",
      "jest-mock-extended": "^3.0.1",
      "nodemon": "^2.0.20",
      "prettier": "2.7.1",
      "prisma": "^4.3.1",
      "reflect-metadata": "^0.1.13",
      "supertest": "^6.3.0",
      "swagger-jsdoc": "^6.2.5",
      "ts-node": "^10.9.1",
      "typescript": "^4.8.3"
   },
   "dependencies": {
      "@prisma/client": "4.3.1",
      "@types/node": "^18.7.18",
      "dotenv-cli": "^6.0.0",
      "express": "^4.18.1",
      "express-validator": "^6.14.2",
      "swagger-ui-express": "^4.5.0"
   }
}
